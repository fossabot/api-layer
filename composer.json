{
    "name": "glocurrency/api-layer",
    "description": "Basic API package.",
    "keywords": ["laravel", "api", "passport"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/glocurrency/api-layer/issues",
        "source": "https://github.com/glocurrency/api-layer"
    },
    "authors": [
        {
            "name": "Ivan Stasiuk",
            "email": "brokeyourbike@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2",
        "goldspecdigital/laravel-eloquent-uuid": "^7.0",
        "laravel/passport": "^9.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.4|^5.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "Glocurrency\\ApiLayer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Glocurrency\\ApiLayer\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Glocurrency\\ApiLayer\\ApiLayerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
